                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <StackPane alignment="BOTTOM_CENTER" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="357.0" fitWidth="388.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@tree%20before.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="userFirstNameString" alignment="CENTER" prefHeight="53.0" prefWidth="52.0" text="Label" />
